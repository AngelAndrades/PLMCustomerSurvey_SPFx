{
  "$schema": "https://developer.microsoft.com/json-schemas/spfx-build/config.2.0.schema.json",
  "version": "2.0",
  "bundles": {
    "plm-survey-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/plmSurvey/PlmSurveyWebPart.js",
          "manifest": "./src/webparts/plmSurvey/PlmSurveyWebPart.manifest.json"
        }
      ]
    }
  },
  "externals": {
    "jquery": {
      "path": "https://code.jquery.com/jquery-3.5.1.min.js",
      "globalName": "jQuery"
    }
  },
  "localizedResources": {
    "PlmSurveyWebPartStrings": "lib/webparts/plmSurvey/loc/{locale}.js",
    "PropertyControlStrings": "node_modules/@pnp/spfx-property-controls/lib/loc/{locale}.js"
  }
}